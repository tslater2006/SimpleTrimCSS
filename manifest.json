{
  "manifest_version": 2,
  "name": "Trim Page CSS",
  "version": "1.0",
  "description": "Save only used CSS rules of a web page",
  "permissions": ["activeTab", "downloads"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Save Page CSS"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}